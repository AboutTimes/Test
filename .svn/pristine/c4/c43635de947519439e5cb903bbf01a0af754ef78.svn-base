package com.oracle.ch22;

import java.io.File;

public class SendsWithTest {
	public static void main(String[] args) {
		File file = new File("E:\\java\\5.10");
		// 判断后缀名
		String[] f = file.list();
		for(String name:f){
			//判断如果是以.java结尾的文件我们就输出
			if(name.endsWith(".java")){
				System.out.println(name);
			}
		}
	}
}
